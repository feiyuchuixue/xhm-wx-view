/* 查看更多评论 */

.cover_box {
    position: fixed;
    width: 100%;
    height: 100%;
    z-index: 2;
    background-color: rgba(0, 0, 0, 0.5);
    top: 0;
    left: 0;
}

.overlay_box {
    position: fixed;
    width: 100%;
    box-sizing: border-box;
    z-index: 3;
    padding: 30rpx 40rpx;
    height: 800rpx;
    background-color: #f5f5f5;
    bottom: 0;
    overflow: scroll;
}

.overlay_title {
    position: relative;
    height: 48rpx;
}

.overlay_title text {
    font-size: 34rpx;
}

.overlay_title image {
    position: absolute;
    right: 0;
    width: 36rpx;
    height: 36rpx;
    top: 6rpx;
}

.textarea_box {
    height: 213rpx;
    border-radius: 10rpx;
    border: 1rpx solid rgb(206, 206, 206);
