var sub = function (val) {
  if (val.length == 0 || val == undefined) {
    return;
  }
  if (val.length > 45) {
    return val.substring(0, 44);
  } else {
    return val;
  }
}
module.exports.sub = sub;

var toDouble = function(value){
  return value.toFixed(2)
}
module.exports.toDouble = toDouble;
